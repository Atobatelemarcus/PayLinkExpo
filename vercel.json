{
  "version": 2,
  "builds": [
    {
      "src": "Exclesior/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "expo-pay/vite.config.*",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "expo-pay/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Exclesior/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "expo-pay/dist/$1"
    }
  ]
}
